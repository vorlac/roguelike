{
  "BuildIncremental": {
    "Enabled": false
  },
  "ExcludedItems": [
    ".git*",
    ".idea",
    ".clang-*",
    ".vscode",
    "*.md",
    "!.natvis",
    "!.out*",
    "!.out/*",
    "!.out/build/*",
    "!.clang-format",
    "!README.md"
  ],
  "Indexing": {
    "Enabled": true
  },
  "Logger": {
    "EnableVsLogging": false,
    "LoggingLevel": "Info"
  }
}
